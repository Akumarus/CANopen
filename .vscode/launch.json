{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Stlink debug stm32f103",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "executable": "${workspaceFolder}/examples/stm32f103/build/Debug/can_f103.elf",
            "device": "STM32F103C8",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f1x.cfg"
            ],
            "runToEntryPoint": "main",
            "gdbPath": "gdb-multiarch",
            "svdFile": "${workspaceFolder}/examples/stm32f103/STM32F103.svd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 1
            }
        },
        {
            "name": "Jlink debug stm32f103",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "jlink",
            "serverpath": "JLinkGDBServer",
            "executable": "${workspaceFolder}/examples/stm32f103/build/Debug/can_f103.elf",
            "device": "STM32F103C8",
            "interface": "swd",
            "runToEntryPoint": "main",
            "gdbPath": "gdb-multiarch",
            "svdFile": "${workspaceFolder}/examples/stm32f103/STM32F103.svd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 2
            }
        },
        {
            "name": "OpenOCD debug stm32f103",
            "cwd": "${workspaceFolder}",
            "type": "cortex-debug",
            "executable": "${workspaceFolder}/examples/stm32f103/build/Debug/can_f103.elf",
            "request": "launch",
            "servertype": "openocd",
            "interface": "swd",
            "serialNumber": "",
            "runToEntryPoint": "main",
            "svdFile": "STM32F103.svd",
            "device": "STM32F103CB",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 1
            },
            "configFiles": [
                "interface/stlink-v2.cfg",
                "target/stm32f1x.cfg"
            ]
        }
    ]
}