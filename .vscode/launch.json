{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug stm32f103",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "executable": "${workspaceFolder}/examples/stm32f103/build/can_f103.elf",
            "device": "STM32F103C8",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f1x.cfg"
            ],
            "runToEntryPoint": "main",
            "gdbPath": "gdb-multiarch",
            "svdFile": "${workspaceFolder}/examples/stm32f103/STM32F103.svd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 1
            }
        }
    ]
}